{
  "name": "glpk.js",
  "version": "4.0.1",
  "description": "GLPK for node & browser",
  "keywords": [
    "LP",
    "MILP",
    "solver",
    "linear programming",
    "linear optimization"
  ],
  "main": "dist/glpk.js",
  "browser": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:em && npm run build:js",
    "build:em": "make all",
    "build:js": "rollup -c rollup.config.js --worker && node src/pack.js && rollup -c rollup.config.js",
    "clean": "make clean",
    "test": "node test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelgruadv/glpk.js.git"
  },
  "author": "Samuel Gru",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/samuelgruadv/glpk.js/issues"
  },
  "homepage": "https://github.com/samuelgruadv/glpk.js#readme",
  "dependencies": {
    "pako": "^2.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "almost-equal": "^1.1.0",
    "rollup": "^2.57.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "tape": "^5.3.1"
  },
  "types": "dist/glpk.d.ts"
}
